{
  "manifest_version": 3,
  "name": "Open all",
  "version": "1.0.0",
  "description": "Open all hyperlinks within selected text in new tabs",
  "permissions": ["contextMenus", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
